<div class="jumbotron">
  <h2>Need To Meet - Find a time that works for everyone!! </h2>
</div>
<form id= "event_details">
  <div class="form-group">
    <label for="FormControlTextarea1">Enter your Event Title</label>
    <textarea id="title" type="text" name="event_title" class="form-control" row="1" style="height:30px;"></textarea>
  </div>
  <button id="create_event_button" type="button" class="btn btn-primary">Submit</button>
  <!-- </form> -->
  <center id="calender"><img align="middle" src="images/calendar-image.gif"></center>
  <div class="form-group">
    <section class="new-event" id= "event_detail_section">
      <!-- <form id= "event_details"> -->
        <div class="form-group">
          <label for="FormControlTextarea2">Enter your name</label>
          <textarea class="form-control" name="creator_name" id="creator_name" rows="1"></textarea>
        </div>
        <div class="form-group">
          <label for="FormControlInput1">Email address</label>
          <input type="email" class="form-control" name="creator_email" id="creator_email" placeholder="name@example.com">
        </div>
        <div class="form-group">
          <label for="FormControlTextarea3">Enter Event Description</label>
          <textarea class="form-control"  name="event_description" id="event_description" rows="2"></textarea>
        </div>
          <div class="form-group">
            <label for="FormControlTextarea4">Enter Event's Location</label>
            <textarea class="form-control" name="event_location" id="location" rows="1"></textarea>
          </div>
          <div class="form-inline">
            <div class="form-group">
              <label for="start_date">Enter start Date and Time</label>
                <div class='input-group date' id='datetimepicker1'>
                    <input type='text' class="form-control" id="start_date" name="datein" readonly />
                    <span class="input-group-addon">
                        <span class="glyphicon glyphicon-calendar"></span>
                    </span>
                </div>
            </div>
            <div class="form-group">
              <label for="end_date"  id="datelabel1">Enter end Date and Time</label>
                <div class='input-group date' id='datetimepicker2'>
                    <input type='text' class="form-control" id="end_date" name="dateout" readonly />
                    <span class="input-group-addon">
                        <span class="glyphicon glyphicon-calendar"></span>
                    </span>
                </div>
            </div>
            <div class="form-group"><button id="add_time" type="button" class="btn btn-primary">Add</button>
            </div>
          </div>
          <div class="form-group">
    <table id="time-table" class="table" style="display:none;">
      <thead>
        <tr>
          <th>Start Time</th>
          <th>End Time</th>
          <th></th>
        </tr>
      </thead>
      <tbody id="table-body">
        <tr>
        </tr>
      </tbody>
    </table>
    </div>
         <br><br>
         <button id="generate_url" type="input" class="btn btn-primary">Generate Event URL</button>
      </form>
    </section>
